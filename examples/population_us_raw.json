{
  "$id": "https://json-unify.github.io/examples/population_us_raw.json",
  "$schema": "https://json-unify.github.io/schemas/v1.json",
  "$comment": "From https://github.com/jdorfman/awesome-json-datasets#population",
  "title": "Historical Population in the US (Since 1960)",
  "dataset": "https://json-unify.github.io/examples/datasets/population.json",
  "type": "object",
  "required": [ "indicator", "country", "countryiso3code", "date", "value", "unit", "obs_status", "decimal" ],
  "additionalProperties": false,
  "properties": {
    "indicator": {
      "type": "object",
      "required": [ "id", "value" ],
      "additionalProperties": false,
      "properties": {
        "id": { "type": "string" },
        "value": { "type": "string" }
      }
    },
    "country": {
      "type": "object",
      "required": [ "id", "value" ],
      "additionalProperties": false,
      "properties": {
        "id": { "type": "string" },
        "value": { "type": "string" }
      }
    },
    "countryiso3code": {
      "type": "string"
    },
    "date": {
      "type": "string"
    },
    "value": {
      "type": "integer"
    },
    "unit": {
      "type": "string"
    },
    "obs_status": {
      "type": "string"
    },
    "decimal": {
      "type": "integer"
    }
  }
}
